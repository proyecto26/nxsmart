{
  "version": 2,
  "projects": {
    "@nxsmart-truffle": {
      "root": "packages/@nxsmart/truffle",
      "sourceRoot": "packages/@nxsmart/truffle/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/@nxsmart/truffle/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/@nxsmart/truffle"],
          "options": {
            "jestConfig": "packages/@nxsmart/truffle/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/@nxsmart/truffle",
            "tsConfig": "packages/@nxsmart/truffle/tsconfig.lib.json",
            "packageJson": "packages/@nxsmart/truffle/package.json",
            "main": "packages/@nxsmart/truffle/src/index.ts",
            "assets": [
              "packages/@nxsmart/truffle/*.md",
              {
                "input": "./packages/@nxsmart/truffle/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/@nxsmart/truffle/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/@nxsmart/truffle",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/@nxsmart/truffle",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "@nxsmart-truffle-e2e": {
      "root": "e2e/@nxsmart/truffle-e2e",
      "projectType": "application",
      "sourceRoot": "e2e/@nxsmart/truffle-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "@nxsmart-truffle:build",
            "jestConfig": "e2e/@nxsmart/truffle-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
